programa {
  funcao inicio() {
    inteiro idade
    inteiro salario
    
    escreva("Qual a sua idade?")
    leia(idade)

    escreva("Qual seu salario?")
    leia(salario)

    se(idade>=20 e salario>=1500){
      escreva("Você está apto para o intercâmbio!")    
      } senao{
        escreva("Você não está apto para o intercâmbio!")
      }

  }
}


