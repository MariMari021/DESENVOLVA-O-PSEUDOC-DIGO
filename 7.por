programa {
  funcao inicio() {
    inteiro valor
    inteiro soma
    inteiro somaDois

    escreva("Informe um valor: ")
    leia(valor)

    se(valor<10){
      soma = valor + 5 - 8 * 6 / 2
      escreva("O resultado é "+soma)
    } senao {
      somaDois = valor + 9 - 14
      escreva("O resultado é "+somaDois)
    }



  }
}

